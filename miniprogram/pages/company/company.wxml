<view class="page">


  <!--main.wxml-->
  <view class="container">
    <!--轮播图-->
    <swiper class="home-swiper" indicator-dots="true" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for-items="{{lunboData}}">
        <swiper-item>
          <image src="{{item.imgurl}}" class="slide-image"/>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <i-collapse name="{{name}}">
    <i-collapse-item title="律所简介" class="company-content" name="name1">
      <view slot="content">
        {{queryResult.company}}
      </view>
    </i-collapse-item>
    <i-collapse-item title="律所荣誉" class="company-content" name="name2">
      <view slot="content">
        {{queryResult.reward}}
      </view>
    </i-collapse-item>
    <i-collapse-item title="律所服务企业" class="company-content" name="name3">
      <view slot="content">
        {{queryResult.pal}}
      </view>
    </i-collapse-item>
    <i-collapse-item title="服务范围" class="company-content" name="name4">
      <view slot="content">
        {{queryResult.about}}
      </view>
    </i-collapse-item>
  </i-collapse>
</view>